{
  "name": "Prattle",
  "repository": "https://github.com/mble/prattle",
  "scripts": {
    "postdeploy": "bundle exec rake db:setup"
  },
  "env": {
    "RAILS_ENV": "production",
    "RACK_ENV": "production",
    "RAILS_SERVE_STATIC_FILES": "true"
  },
  "addons": [
    {
      "plan": "heroku-postgresql",
      "options" : {
          "version": "9.6"
      }
    }
  ]
}
